{
  "name": "execut/yii2-people-finder",
  "description": "Extension for search people via social networks. Now supported only ok.ru",
  "keywords": ["yii2 ok.ru", "yii2 social networks", "yii2 people finder"],
  "homepage": "https://github.com/execut/yii2-people-finder",
  "type": "yii2-extension",
  "license": "Apache-2.0",
  "support": {
    "issues": "https://github.com/execut/yii2-people-finder/issues?state=open",
    "wiki": "https://github.com/execut/yii2-people-finder/wiki",
    "source": "https://github.com/execut/yii2-people-finder"
  },
  "authors": [
    {
      "name": "Mamaev Yuriy (eXeCUT)"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4",
    "yiisoft/yii2": "@dev",
    "guzzlehttp/guzzle": "^7.0",
    "modularsoftware/php-gedcom": "^1.1",
    "ext-dom": "*",
    "vkcom/vk-php-sdk": "dev-master"
  },
  "require-dev": {
    "codeception/codeception": "^4.0",
    "codeception/module-asserts": "^1.0",
    "codeception/module-yii2": "^1.0",
    "codeception/module-filesystem": "^1.0",
    "codeception/verify": "~0.5.0 || ~1.1.0"
  },
  "config": {
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "extra": {
    "bootstrap": "execut\\peopleFinder\\Bootstrap\\Auto"
  },
  "autoload": {
    "psr-4": {
      "execut\\peopleFinder\\": "src"
    }
  }
}
